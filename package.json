    "@types/react-autosuggest": "^9.3.6",
    "@types/react-redux": "^7.0.1",
    "@types/react-responsive": "^3.0.2",
    "autosuggest-highlight": "^3.1.1",
    "enzyme-to-json": "^3.3.5",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.6.3",
    "react-fontawesome": "^1.6.1",
    "react-moment": "^0.8.4",
    "react-redux": "^6.0.0",
    "react-responsive": "^6.0.1",
