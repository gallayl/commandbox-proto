    "jest": "^23.6.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
